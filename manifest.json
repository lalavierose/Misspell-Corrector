{
  "manifest_version": 3,
  "version": "2.0",
  "name": "Misspell Corrector",
  "description": "An chrome extension to correct mistypes in the wrong language!",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite"
  ],
  "background": {
    "service_worker": "js/app.js"
  },
  "sandbox":{
    "pages":["index.html"]
  }
}
